{
  "expo": {
    "name": "FrangoNosso",
    "slug": "FrangoNosso",
    "version": "1.1.1",
    "orientation": "portrait",
    "icon": "./src/Images/fran.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./src/Images/fran.png",
      "resizeMode": "contain",
      "backgroundColor": "#111111"
    },
    "plugins": [
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow $(PRODUCT_NAME) to use your location."
        }
      ],
      "expo-font"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/Images/fran.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION",
        "android.permission.FOREGROUND_SERVICE"
      ],
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyAnQYEHDV7yLhUtqX_rKPCL3NjldMJ94ro"
        }
      },
      "package": "com.lmouraDev.FrangoNosso"
    },
    "web": {
      "favicon": "./src/Images/fran.png"
    },
    "extra": {
      "eas": {
        "projectId": "21b62601-11ca-4aa2-a8d2-18a1278286a1"
      }
    }
  }
}
